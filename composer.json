{
    "name": "ubitcorp/laravel-settings",
    "description": "It provides a settings model that can be used with trait on any model for Laravel project",
    "keywords": ["ubitcorp", "laravel-settings", "settings", "hasan civelek"],
    "license": "MIT",
    "homepage": "https://github.com/ubitcorp/laravel-settings",
    "authors": [{
        "name": "Hasan Civelek",
        "email": "hasan@ubit.com.tr",
        "role": "Developer"
    }],
    "require": {
        "php": "~7.1",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x"
    },
    "autoload": {
        "psr-4": {
            "ubitcorp\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ubitcorp\\Settings\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ubitcorp\\Settings\\ServiceProvider"
            ],
            "aliases": {
                "Settings": "ubitcorp\\Settings\\Facade"
            }
        }
    }
}